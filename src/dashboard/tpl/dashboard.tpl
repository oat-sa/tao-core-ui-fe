<div class="readiness-dashboard-container">
    <h3 class="dashboard-header">{{headerText}}</h3>
    <section>
        <div class="dashboard-loading" style="display: none;">
            <p class="dashboard-text">
                {{loadingText}}
            </p>
        </div>
        <div class="dashboard-metrics_container"></div>
        <div class="dashboard-warning" style="display: none;">
            <p class="dashboard-text">
                {{warningText}}
            </p>
        </div>
    </section>
</div>
