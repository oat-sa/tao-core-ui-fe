<table class="file">
    <tbody>
        <tr><th>{{__ 'Identifier'}}</th><td>{{id}}</td></tr>
        <tr><th>{{__ 'Username'}}</th><td>{{login}}</td></tr>
        <tr><th>{{__ 'First Name'}}</th><td>{{firstname}}</td></tr>
        <tr><th>{{__ 'Last Name'}}</th><td>{{lastname}}</td></tr>
        <tr><th>{{__ 'Email address'}}</th><td>{{email}}</td></tr>
        <tr><th>{{__ 'Phone number'}}</th><td>{{phone}}</td></tr>
        <tr><th>{{__ 'Country'}}</th><td>{{country}}</td></tr>
        <tr><th>{{__ 'State'}}</th><td>{{state}}</td></tr>
        <tr><th>{{__ 'City'}}</th><td>{{city}}</td></tr>
        <tr><th>{{__ 'Zip Code'}}</th><td>{{zipcode}}</td></tr>
        <tr><th>{{__ 'Street'}}</th><td>{{street}}</td></tr>
        <tr><th>{{__ 'Creation At'}}</th><td>{{created_at}}</td></tr>
    </tbody>
</table>
